{
  "env": {
    "development": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "plugins": ["styled-jsx-plugin-postcss"]
            }
          }
        ],
        "@zeit/next-typescript/babel"
      ]
    },
    "production": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "plugins": ["styled-jsx-plugin-postcss"]
            }
          }
        ],
        "@zeit/next-typescript/babel"
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": { "modules": "commonjs" },
            "styled-jsx": {
              "plugins": ["styled-jsx-plugin-postcss"]
            }
          }
        ],
        "@zeit/next-typescript/babel"
      ]
    }
  }
}
